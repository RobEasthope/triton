{
  "name": "storybook-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn run tailwind:watch\" \"yarn run storybook\"",
    "storybook": "start-storybook -p 6006 --quiet --no-open",
    "tailwind": "npx tailwindcss -i ./styles/tailwind-globals.css -o ./styles/tailwind.css --minify",
    "tailwind:watch": "npx tailwindcss -i ./styles/tailwind-globals.css -o ./styles/tailwind.css --watch",
    "build": "build-storybook",
    "prebuild": "yarn run tailwind",
    "lint": "eslint . --color",
    "lint:fix": "eslint . --color --fix",
    "clean": "rm -rf node_modules .turbo"
  },
